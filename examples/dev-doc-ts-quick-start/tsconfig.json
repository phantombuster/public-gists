{
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "es5",
		"module": "esNext",
		"outDir": "dist",
		"lib": ["dom", "es2015", "es2015.promise", "es2016", "es2017", "es2018", "es2019"],
		"esModuleInterop": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"alwaysStrict": true,
	},
	"include": ["./src"]
}